<?xml version="1.0" encoding="utf-8"?>
<browserconfig>
  <msapplication>
    <tile>
      <square70x70logo src="/tile-small.png"/>
      <square150x150logo src="/tile-medium.png"/>
      <wide310x150logo src="/tile-wide.png"/>
      <square310x310logo src="/tile-large.png"/>
